		<library name="kernel" location="${GOBO_LIBRARY}/library/kernel/library.ecf" readonly="false"/>
		<library name="kernel_test_unit" location="${GOBO_LIBRARY}/library/kernel/test/unit/system.ecf" readonly="false"/>
		<library name="math" location="${GOBO_LIBRARY}/library/math/library.ecf" readonly="false"/>
		<library name="math_example_decimal_telco" location="${GOBO_LIBRARY}/library/math/example/decimal/telco/system.ecf" readonly="false"/>
		<library name="math_test_example" location="${GOBO_LIBRARY}/library/math/test/example/system.ecf" readonly="false"/>
		<library name="math_test_unit_decimal_acceptance" location="${GOBO_LIBRARY}/library/math/test/unit/decimal/acceptance/system.ecf" readonly="false"/>
		<library name="pattern" location="${GOBO_LIBRARY}/library/pattern/library.ecf" readonly="false"/>
		<library name="pattern_example_singleton_do_nothing" location="${GOBO_LIBRARY}/library/pattern/example/singleton/do_nothing/system.ecf" readonly="false"/>
		<library name="pattern_test_example" location="${GOBO_LIBRARY}/library/pattern/test/example/system.ecf" readonly="false"/>
		<library name="pattern_test_unit" location="${GOBO_LIBRARY}/library/pattern/test/unit/system.ecf" readonly="false"/>
		<library name="structure" location="${GOBO_LIBRARY}/library/structure/library.ecf" readonly="false"/>
		<library name="structure_test_unit" location="${GOBO_LIBRARY}/library/structure/test/unit/system.ecf" readonly="false"/>
		<library name="thread" location="${GOBO_LIBRARY}/library/thread/library_${GOBO_EIFFEL}.ecf" readonly="false"/>
