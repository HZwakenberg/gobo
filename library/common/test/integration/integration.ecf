		<library name="parse" location="${GOBO_LIBRARY}/library/parse/library.ecf" readonly="false"/>
		<library name="parse_skl" location="${GOBO_LIBRARY}/library/parse/library_skl.ecf" readonly="false"/>
		<library name="regexp" location="${GOBO_LIBRARY}/library/regexp/library.ecf" readonly="false"/>
		<library name="string" location="${GOBO_LIBRARY}/library/string/library.ecf" readonly="false"/>
		<library name="test" location="${GOBO_LIBRARY}/library/test/library.ecf" readonly="false"/>
